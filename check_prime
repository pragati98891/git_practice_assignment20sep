function primeLoopCheck(num){
		
for (let i=1; i<=num; i++){
	   
 if (check(i) && i%2==1){
	       
console.log(i);
	    
}
	
} 
	
function check(N){
	   
 if (N<=1){
	      
  return false;
	    
}
	   
 for (let i=2; i<=N**0.5; i++){
	       
 if (N%i===0){
	           
 return false;
	       
 }
	
}
	
return true;
  
 
}

}